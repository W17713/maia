{
"name":"RA Chrome Extension",
"version":"0.0.1",
"description":"Used to copy and organize text from web pages",
"browser_action":{
	"default_icon":"icons/icon.png",
	"default_popup":"dashboard.html"
},

	
"content_scripts":[{
"matches":["<all_urls>"],
"js":["dashboard.js"]
}],
"background":{
	"scripts":["getSelectedText.js"],
	"persistent":true
	},
	
"permissions":["activeTab","storage"],
"manifest_version":2
}

